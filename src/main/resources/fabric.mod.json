{
  "schemaVersion": 1,
  "id": "atbyw",
  "version": "${version}",

  "name": "All The Blocks You Want (ATBYW)",
  "description": "All The Blocks You Want (ATBYW) is a Minecraft Fabric mod that aims to make Minecraft's block lineup more rich and varied, it adds building blocks, utility blocks and redstone components of all sorts.",
  "authors": [
    "Azagwen"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/all-the-blocks-you-want",
    "sources": "https://github.com/Azagwen/ATBYW"
  },

  "license": "CC0",
  "icon": "assets/atbyw/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.azagwen.atbyw.init.AtbywMain"
    ],
    "client": [
      "net.azagwen.atbyw.init.AtbywClient"
    ]
  },
  "mixins": [
    "atbyw.mixins.json"
  ],

  "depends": {
    "fabricloader": "*",
    "fabric": "*",
    "minecraft": "*"
  },
  "suggests": {
    "flamingo": "*"
  }
}
